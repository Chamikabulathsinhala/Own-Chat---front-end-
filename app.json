{
  "expo": {
    "name": "OwnChat",
    "slug": "OwnChat",
    "scheme": "OwnChat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Splash2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Splash2.png",
      "resizeMode": "native",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Splash2.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ]
  }
}
